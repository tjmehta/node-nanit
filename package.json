{
  "name": "nanit-camera",
  "version": "1.6.3",
  "description": "Description",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/esm/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "scripts": {
    "start": "node -r dotenv/config dist/esm/app.mjs",
    "prepublish": "npm run build",
    "build": "npm run build-ts && npm run build-proto",
    "build-ts": "npm run _build_ts_esm && npm run _build_ts_cjs",
    "_build_ts_esm": "tsc -p tsconfig.json && rewrite-esm dist/esm",
    "_build_ts_cjs": "tsc -p tsconfig-cjs.json && rewrite-cjs dist/cjs",
    "clean-proto": "rm -rf ./src/proto/*.js && rm -rf ./src/proto/*.ts",
    "build-proto": "npm run clean-proto && npm run _build-proto-js && npm run _build-proto-ts && npm run _copy-proto",
    "_build-proto-js": "npm run __build-proto-esm-js && npm run __build-proto-cjs-js",
    "__build-proto-esm-js": "pbjs -t static --es6 ./src/proto/nanit.proto > ./src/proto/nanit.esm.js && npm run ___build-proto-esm-js-import",
    "___build-proto-esm-js-import": "echo 'import $protobuf from \"protobufjs/minimal.js\";' > ./src/proto/nanit.js && cat ./src/proto/nanit.esm.js >> ./src/proto/nanit.js && rm -f ./src/proto/nanit.esm.js",
    "__build-proto-cjs-js": "pbjs -t static-module ./src/proto/nanit.proto > ./src/proto/nanit.cjs",
    "_build-proto-ts": "pbts ./src/proto/nanit.js > ./src/proto/nanit.d.ts",
    "_copy-proto": "npm run __copy-proto-esm && npm run __copy-proto-cjs",
    "__copy-proto-esm": "cp -R ./src/proto/ ./dist/esm/proto/ && rm -f ./dist/esm/proto/nanit.cjs",
    "__copy-proto-cjs": "cp -R ./src/proto/ ./dist/cjs/proto/ && cp ./dist/cjs/proto/nanit.cjs ./dist/cjs/proto/nanit.js && rm -f ./dist/cjs/proto/nanit.cjs",
    "test": "node --experimental-vm-modules --experimental-specifier-resolution=node node_modules/jest/bin/jest.js --detectOpenHandles",
    "test-update": "node --experimental-vm-modules --experimental-specifier-resolution=node node_modules/jest/bin/jest.js -u",
    "test-debug": "node --experimental-vm-modules --experimental-specifier-resolution=node --inspect-brk node_modules/jest/bin/jest.js",
    "censor-snapshots": "node ./src/scripts/censorTestSnapshotsAndRecordings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjmehta/node-nanit-camera.git"
  },
  "keywords": [],
  "author": "Tejesh Mehta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjmehta/node-nanit-camera/issues"
  },
  "homepage": "https://github.com/tjmehta/node-nanit-camera#readme",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@pollyjs/adapter-fetch": "^6.0.6",
    "@pollyjs/adapter-node-http": "^6.0.6",
    "@pollyjs/core": "^6.0.6",
    "@pollyjs/persister-fs": "^6.0.6",
    "@types/jest": "^25.2.3",
    "@types/koa": "^2.15.0",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^20.11.19",
    "@types/node-media-server": "^2.3.6",
    "@types/p-defer": "^2.0.0",
    "@types/rimraf": "^4.0.5",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.5",
    "esm": "^3.2.25",
    "fast-abort-controller": "^4.0.3",
    "jest": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "nock": "^13.5.3",
    "node-fetch": "^3.3.2",
    "prettier": "^2.8.8",
    "protobufjs-cli": "^1.1.2",
    "replace-in-file": "^7.1.0",
    "rewrite-cjs-esm-paths": "^1.0.3",
    "rimraf": "^5.0.7",
    "segfault-handler": "^1.3.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@koa/bodyparser": "^5.1.1",
    "@koa/router": "^12.0.1",
    "abortable-timeout": "^3.0.4",
    "abstract-http-server": "^2.0.11",
    "abstract-startable": "^4.1.2",
    "async-mqtt": "^2.6.3",
    "baseerr": "^2.0.4",
    "composite-abort-controller": "^3.0.5",
    "env-var": "^7.5.0",
    "koa": "^2.15.3",
    "long": "^5.2.3",
    "memoize-concurrent": "^2.0.3",
    "mqtt": "^5.10.1",
    "node-media-server": "^2.7.0",
    "p-defer": "^3.0.0",
    "promise-backoff": "^2.0.4",
    "protobufjs": "^7.2.6",
    "race-abort": "^3.0.3",
    "rxjs": "^7.8.1",
    "simple-api-client": "^4.0.5",
    "timeout-then": "^2.0.0",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  }
}
